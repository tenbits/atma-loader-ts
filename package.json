{
  "name": "atma-loader-ts",
  "version": "1.1.5",
  "description": "TypeScript compiler for `atma-io`, `IncludeJS` and `atma-server`.",
  "author": "Alex Kit (alex.kit@atmajs.com)",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "app-bundler"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tenbits/atma-loader-ts"
  },
  "dependencies": {
    "atma-io-middleware-base": "^1.0.26",
    "atma-loader-stacktrace": "^0.1.1",
    "atma-utils": "^0.1.13",
    "typescript": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^10.1.3",
    "app-bundler": "0.0.47",
    "atma-loader-ts": "1.0.0"
  },
  "app-bundler": {
    "file": "src/index.ts",
    "outputMain": "index.js",
    "package": {
      "module": "commonjs",
      "commonjs": {
        "output": "simplified"
      }
    },
    "defaultExtensions": {
      "js": "ts"
    },
    "middlewares": {
      "ts": [
        "atma-loader-ts:read"
      ]
    },
    "dynamicDependencies": [
      "typescript",
      "atma-io-middleware-base"
    ]
  },
  "atma": {
    "settings": {
      "atma-loader-ts": {
        "typescript": {
          "compilerOptions": {
            "module": "AMD"
          }
        }
      }
    }
  }
}
